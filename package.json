{
  "name": "diskclone",
  "version": "1.0.0",
  "description": "copy disk's files to usb",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . DiskClone --platform linux --arch x64 --version 1.1.3  --prune  --out=dist/ --overwrite  --icon =app/assets/img/appledisk.png --prune   --ignore=node_modules/electron-prebuilt --ignore=node_modules/electron-packager --ignore =.git",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "diskclone"
  ],
  "dependencies": {
