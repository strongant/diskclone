    "gulp": "^3.9.1",
    "gulp-minify-css": "^1.2.4",
    "gulp-uglify": "^1.5.3",
    "gulp-usemin": "^0.3.23",
    "log4js": "^0.6.36",
    "mysql": "^2.10.2",
    "q": "^1.4.1",
    "xml2js": "^0.4.16",
    "xml2json": "^0.9.1"
  },
